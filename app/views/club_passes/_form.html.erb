<%= form_with(model: club_pass, local: true) do |f| %>
  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :start_date %>
  <%= f.date_field :start_date %>

  <%= f.label :end_date %>
  <%= f.date_field :end_date %>

  <%= f.submit %>
<% end %>
